<Window x:Class="client.IPCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client"
        mc:Ignorable="d"
        Title="IPCheck" Height="450" Width="800">
    <Grid>
		<TextBlock HorizontalAlignment="Left" Margin="301,63,0,0" TextWrapping="Wrap" Text="接続するIPアドレスを選択してください" VerticalAlignment="Top" RenderTransformOrigin="0.168,-0.015"/>
		<Button Content="Button" HorizontalAlignment="Left" Margin="340,337,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.53,0.845" Click="Button_Click"/>
		<ListBox ItemsSource="{Binding Path=Data}" HorizontalAlignment="Left" Height="100" Margin="328,134,0,0" VerticalAlignment="Top" Width="100"/>

	</Grid>
</Window>
